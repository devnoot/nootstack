{
  "singleQuote": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "quoteProps": "consistent",
  "semi": false,
  "trailingComma": "all",
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "prettier-plugin-css-order",
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ]
}
